button {
	background-color: #FF2D55;
	color: white;
	margin: 20rpx;
}

label {
	color: #FF2D55;
	margin-right: 25rpx;
}

.row {
	display: flex;
	flex-direction: row;
}

.col {
	display: flex;
	flex-direction: column;
}

.box {
	border-bottom: 1rpx solid silver;
	margin: 20rpx;
	padding: 10rpx;
}

form,
textarea {
	width: 100%;
}

input {
	width: 100rpx;
}
